    "karma": "2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-webpack": "4.0.0-beta.0",
    "preact": "8.3.1",
    "source-map-loader": "0.2.3",
    "ts-loader": "5.0.0",
